	<queryString>
		<![CDATA[SELECT DISTINCT
    e.id AS evento_id,
    tipo_evento.descricao AS evento_tipo,
    e.titulo AS evento_titulo,
    e.carga_horaria AS evento_carga_horaria,
    e.data_inicio_previsto AS evento_data_inicio_previsto,
    e.data_fim_previsto AS evento_data_fim_previsto,
    e.data_inicio_pre_inscricao AS data_inicio_pre_inscricao,
    e.data_fim_pre_inscricao AS data_fim_pre_inscricao,
    e.objetivo_geral AS evento_objetivo_geral,
    e.objetivo_especifico AS evento_objetivo_especifico,
    e.conteudo AS evento_conteudo,
    e.modulo_unico AS evento_modulo_unico,
    e.resultado_esperado AS evento_resultado_esperado,
    e.observacoes_publicas AS evento_observacoes_publicas,
    e.link_evento AS evento_link,
    e.link_gravacao AS evento_link_gravacao,
    participante.nome AS evento_responsavel,
    (
        SELECT
            LISTAGG(pe2.descricao,', ') WITHIN GROUP(ORDER BY epj2.provedor_id)
        FROM
            evento_provedor_join epj2
            INNER JOIN evento e2 ON epj2.evento_id = e2.id
            INNER JOIN provedor_evento pe2 ON epj2.provedor_id = pe2.id
        WHERE
            e2.id = e.id
    ) AS evento_provedor,
    eixo_tematico.descricao AS eixo_tematico_descricao,
    modulo.id AS modulo_id,
    modulo.titulo AS modulo_titulo,
    modulo.carga_horaria AS modulo_carga_horaria,
    modulo.data_inicio AS modulo_data_inicio,
    modulo.data_fim AS modulo_data_fim,
    modulo.hora_inicio_turno1 AS modulo_hora_inicio_turno1,
    modulo.hora_fim_turno1 AS modulo_hora_fim_turno1,
    modulo.hora_inicio_turno2 AS modulo_hora_inicio_turno2,
    modulo.hora_fim_turno2 AS modulo_hora_fim_turno2,
    modulo.hora_inicio_turno3 AS modulo_hora_inicio_turno3,
    modulo.hora_fim_turno3 AS modulo_hora_fim_turno3,
    modulo.frequencia AS modulo_frequencia,
    modulo.nota AS modulo_nota,
    modulo.observacao AS modulo_observacao,
    modalidade.descricao AS modalidade_modulo,
    modalidade.id AS modalidade_id_modulo,    
    localizacao.descricao AS modulo_localizacao_descricao,
    localizacao.endereco AS modulo_localizacao_endereco,
    cidade.nome AS modulo_localizacao_cidade,
    cidade.uf AS modulo_localizacao_uf,
    pais.descricao AS modulo_localizacao_pais
FROM
    evento e
    INNER JOIN tipo_evento ON e.tipo_id = tipo_evento.id
    LEFT  JOIN eixo_tematico ON e.eixo_tematico_id = eixo_tematico.id
    LEFT  JOIN participante ON e.responsavel_evento = participante.id
    INNER JOIN modulo ON e.id = modulo.evento_id
    INNER JOIN modalidade ON modulo.modalidade_id = modalidade.id
    INNER JOIN tipo_localizacao_evento localizacao ON modulo.localizacao_id = localizacao.id
    LEFT  JOIN srh_tb_municipio cidade ON cidade.id = localizacao.cidade_id
    LEFT  JOIN pais ON localizacao.pais_id = pais.id
WHERE
	e.id = $P!{EVENTO_ID}
ORDER BY
    modulo.data_inicio,
    modulo.id]]>
	</queryString>
